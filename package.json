    "prettier": "^3.2.5",
    "vite": "^5.2.11"
  },
  "lint-staged": {
    "*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
