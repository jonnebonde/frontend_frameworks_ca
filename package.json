  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss,md}\"",
